[core]
  editor = vim
	autocrlf = input
[color]
  ui = auto
[color "branch"]
  current = green reverse
  local = green
  remote = yellow
[color "diff"]
  meta = yellow
  frag = magenta
  old = red
  new = green
[color "status"]
  added = green
  changed = yellow
  untracked = red
[push]
  default = simple
[alias]
  st = status
  ci = commit
  br = branch
  co = checkout
  df = diff
  lg = log -p
  lol = log --graph --decorate --pretty=oneline --abbrev-commit
  ls = ls-files
[credential]
  helper = osxkeychain
[commit]
  template = ~/.gitmsg
